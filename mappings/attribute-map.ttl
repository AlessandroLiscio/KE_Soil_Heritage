@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix crml: <http://w3id.org/stlab/crml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix pr: <http://prova.attribute.ex/att#> .

<#AttributesCSV>
  rml:source "./data/Descrizione_campi.csv" ;
  rml:referenceFormulation ql:CSV;
  crml:separator ";".


<#Attributes>
  rml:logicalSource <#AttributesCSV>;

  rr:subjectMap [
    rr:template "https://lisciofortini/attribute/{Campo_ID}";
    rr:class pr:Attribute
  ];

  rr:predicateObjectMap [
    rr:predicate pr:campo;
    rr:objectMap [
      rml:reference "Campo";
      rr:datatype xsd:string
    ]
  ];

	rr:predicateObjectMap [
	rr:predicate pr:descrizione;
	rr:objectMap [
			rml:reference "Descrizione";
			rr:datatype xsd:string
		]
	].




