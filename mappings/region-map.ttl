@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix crml: <http://w3id.org/stlab/crml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix pr: <http://prova.attribute.ex/att#> .


<#RegionsCSV>
	rml:source "./data/Regioni_2012.csv" ;
	rml:referenceFormulation ql:CSV;
	crml:separator ";".


<#Regions>    
	rml:logicalSource <#RegionsCSV>;

  rr:subjectMap [
    rr:template "https://lisciofortini/regions/{COD_REG}";
    rr:class pr:Place
  ];

  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "NOME_Regione";
      rr:datatype xsd:string
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate <https://lisciofortini/attribute/C1>;
    rr:objectMap [
      rml:reference "C1";
      rr:datatype xsd:string
    ]
  ].